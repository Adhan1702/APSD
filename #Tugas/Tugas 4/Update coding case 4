#include <iostream>
using namespace std;

int main() {
    int product = 1;
    for(int i = 1; i <= 9; i += 2) {
        product *= i;
        for(int j = 1; j <= i; j += 2) {
            cout << j;
            if(j != i) {
                cout << " * ";
            }
        }
        cout << " = " << product << endl;
    }
    return 0;
}
